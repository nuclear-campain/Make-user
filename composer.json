{
    "name": "b61/portal-make-user",
    "description": "An Artisan console command to create users in your Laravel application.",
    "license": "MIT",
    "keywords": ["laravel", "artisan", "console"],
    "authors": [
        {
            "name": "Tim Joosten",
            "email": "Tim@activisme.be",
            "homepage": "https://www.activisme.be"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.7.*",
        "illuminate/console": "5.7.*",
        "illuminate/database": "5.7.*",
        "illuminate/auth": "5.7.*",
        "illuminate/notifications": "5.7.*",
        "spatie/laravel-permission": "^2.27"
    },
    "autoload": {
        "psr-4": {
            "ActivismBE\\Artisan\\": "src/"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "^3.7"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "ActivismBE\\Artisan\\MakeUserServiceProvider"
            ]
        }
    }
}
